  node {
    stage('Checkout') {
       checkout scm
    }
    stage('Environment') {
      sh 'git --version'
      echo "Branch: ${env.BRANCH_NAME}"
      sh 'docker -v'
    }
    stage('Build Docker'){
     sh 'docker build -t boxagent .'
    }
    stage('Deploy'){
        sh 'docker tag boxagent 149.91.90.168:5000/boxagent'
        sh 'docker push 149.91.90.168:5000/boxagent'
        }
    }
  
  